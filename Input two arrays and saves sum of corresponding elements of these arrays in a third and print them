#include <iostream>
using namespace std;
int main()
{
	int size,i,j,k,f;
	cout << "Enter The Size of Array" << "\n";
	cin >> size;
	int arr1[size];
	int arr2[size];
	int arr3[size];
	cout << "Enter The Elements of Array One" << "\n";
	for(i=0;i<size;i++)
		{	
			cin >> arr1[i];
		}
	cout << "Enter The Elements of Array Two" << "\n";
	for(j=0;j<size;j++)
		{	
			cin >> arr2[j];
		}
	cout << "Sum of Corresponding Elements" << "\n";
	for(k=0;k<size;k++)
	{
		f = arr1[k]+arr2[k];
		cout << f << "\n";
	}
	return 0;
}
